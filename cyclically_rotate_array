class Compute {
    
    public void rotate(int arr[], int n)
    {
        
        
        int last=arr.length-1;
        int temp=0;
        
        while(last>0){
            
            temp=arr[last];
            arr[last]=arr[last-1];
            arr[last-1]=temp;
            
            last--;
            
        }
        
        
    }
}
