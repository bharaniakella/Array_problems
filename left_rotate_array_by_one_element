public class Spartan {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] a = {1,2,3,4,5,6};
		
		for(int i=0;i<a.length;i++) {
			System.out.print(a[i]+" ");
		}
		
		left_rotate(a);
		
		System.out.println();
			
		for(int i=0;i<a.length;i++) {
			System.out.print(a[i]+" ");
		}
		
	}
	
	public static void left_rotate(int a[]) {
		
			for(int i=1;i<a.length;i++) {
				int temp=a[i];
				a[i]=a[i-1];
				a[i-1]=temp;
			}
		
		
	}

}
